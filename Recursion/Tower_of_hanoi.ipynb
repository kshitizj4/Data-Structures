    "    if n==1:\n",
    "        print(\"Move 1st disk from\", a,\" to \",c)\n",
    "        return\n",
    "    tower_hanoi(n-1,a,c,b)\n",
    "    print(\"Move \",n,\" th disk from\", a,\" to \",c)\n",
    "    tower_hanoi(n-1,b,a,c)\n",
    "    "
